{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "no-console": "error",
        "func-names": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "jest/expect-expect": "off",
        "security/detect-object-injection": "off"
      }
}
